{
  "server": {
    "storage": {
      "type": "local",
      "size_limit": "5 MB",
      "filename_length": 8,
      "local": {
        "path": "/tmp/files",
        "external": "http://localhost:8080/view.html"
      },
      "aws": {
        "bucket": "bucket",
        "storage_class": "STANDARD",
        "baseurl": "https://region.amazonaws.com/bucket/",
        "acl": "public-read"
      }
    }
  }
}
